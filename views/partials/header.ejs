<header id="top-header">
  <span class="siteName">
    <a href="/" title="Return to home page">CSE Motors</a>
  </span>
  <div id="account-management">
  <% if (locals.loggedin) { %>
    <a href="/account" title="Account management">
      Welcome <%= locals.accountData.account_firstname %>
    </a>

    <a href="/account/logout" title="Log out">
      Logout
    </a>

  <% } else { %>

    <a href="/account/login" title="Click to log in">
      My Account
    </a>
  <% } %>
</div>

  </div>
</header>